{
  "name": "styleConfig",
  "styles": {
    "marker-varying": {
      "color": {
        "exact-location": "yellow",
        "near-here": "purple",
        "in_this_country":"grey",
        "in_this_town_city":"pink"
      },
      "hit-radius":16,
      "highlightable-props": [
        {
          "prop-name": "circle-radius",
          "active": 7,
          "inactive": 5,
          "default": 6
        },
        {
          "prop-name": "circle-stroke-width",
          "active": 2,
          "inactive": 1,
          "default": 2
        },
        {
          "prop-name": "circle-stroke-color",
          "active": "white",
          "inactive": "lightgray",
          "default": "white"
        }
      ]
    },
    "marker(HTML MARKERS DEPRICATED)": {
      "backgroundColor": "grey",
      "opacity": ".8",
      "width": "12px",
      "height": "12px",
      "border-radius": "50%",
      "border": "2px solid white",
      "cursor": "pointer"
    }
  },
  "layer-placement": {
    "events": "event-placeholder",
    "geojson": "additional-layer-placeholder"
  },
  "colors": {
    "yellow": {
      "primary": "#FAD40A",
      "inactive": "#d5d26f",
      "mid": "#FEF6CE",
      "light": "#FEFAE8"
    },
    "purple": {
      "primary": "#6013FB",
      "inactive": "#d2befa",
      "mid": "#120232",
      "light": "#FCFBFF"
    },
    "pink":{
      "primary": "#CC0066",
      "inactive": "#CC9999"
    },
    "grey":{
      "primary": "#999",
      "inactive": "#CCC"
    }
  }
}
